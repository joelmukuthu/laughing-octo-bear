<% messages = defined?(@one_time_flash) ? @one_time_flash : flash %>
<% messages.each do |key, value| %>
  <div class="alert alert-<%= key %>">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      <ul>
        <li><%= value %></li>
      </ul>
  </div>
<% end %>